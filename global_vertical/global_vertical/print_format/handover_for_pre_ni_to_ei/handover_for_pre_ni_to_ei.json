 "html": "<table style=\"width:100%; border-collapse: collapse; table-layout: fixed;\" border=\"1\">\n    <!-- HEADER -->\n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">\n            <img src=\"/files/global_logo.png\" alt=\"Logo\" style=\"height:35px;\">\n        </td>\n        <td colspan=\"6\" style=\"text-align:center; font-weight:bold; background:#4a86e8;font-size:16px;\">\n            HAND OVER FROM PRE NI TO EI\n        </td>\n        <td colspan=\"2\" style=\"text-align:center;font-size:10px;font-weight:bold;\">SNO</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.sno or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#f3f3f3;\">\n            SITE NAME\n        </td>\n        <td colspan=\"6\" style=\"text-align:center; \">\n            {{doc.site_name or \"\"}}\n        </td>\n        <td colspan=\"2\" style=\"text-align:center;font-size:10px;font-weight:bold;font-size:12px;background-color:#f3f3f3;\">ORDER NO</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.order_no or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#f3f3f3;\">\n            SITE ADDRESS\n        </td>\n        <td colspan=\"6\" style=\"text-align:center; \">\n            {{doc.site_address or \"\"}}\n        </td>\n        <td colspan=\"2\" style=\"text-align:center;font-size:10px;font-weight:bold;;background-color:#f3f3f3;\">ACCEPTED BY NI</td>\n        <td colspan=\"1\" style=\"font-size:11px;text-align:center;\">{{ '&#x2611;' if doc.yes else '&#x2610;' | safe }}  YES</td>\n        <td colspan=\"1\" style=\"font-size:11px;text-align:center;\">{{ '&#x2611;' if doc.no else '&#x2610;' | safe }}  NO</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center; \"></td>\n        <td colspan=\"1\" style=\"text-align:center; \">PIN CODE</td>\n        <td colspan=\"2\" style=\"text-align:center; \">{{doc.pincode or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;;font-weight:bold;background-color:#f3f3f3;\">CITY</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.city or \"\"}}</td>\n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;;background-color:#f3f3f3\">CONTACT DETAILS</td>\n        <td colspan=\"6\" style=\"text-align:center;font-weight:bold;;background-color:#f3f3f3\">CUSTOMER CONTACT NAME</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;;background-color:#f3f3f3\">MOBILE</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;;background-color:#f3f3f3\">EMAIL</td>\n    </tr>\n    \n     {% for data in doc.contact_details %}\n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">{{data.contact_details or \"\"}}</td>\n        <td colspan=\"6\" style=\"text-align:center;\">{{data.customer_contact_name or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{data.email or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{data.mobile or \"\"}}</td>\n    </tr>\n    {% endfor %}\n   \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">NO OF INSPECTIONS</td>\n        <td colspan=\"3\" style=\"background-color:#434343;color:white;text-align:center;font-weight:bold;border-right:1px solid white;\">1</td>\n        <td colspan=\"2\" style=\"background-color:#434343;color:white;text-align:center;font-weight:bold;border-right:1px solid white;\">2</td>\n        <td colspan=\"1\" style=\"background-color:#434343;color:white;text-align:center;font-weight:bold;\">3</td>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-weight:bold;;background-color:#f3f3f3;font-size:12px;\">DATE OF EI ACCEPTANCE</td>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-weight:bold;;background-color:#f3f3f3\">{{ frappe.utils.format_date(doc.date_of_ei_acceptance, \"dd-MM-yyyy\") }}</td>        \n        \n    </tr>\n    \n     <tr>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">INSPECTION DATE</td>\n        <td colspan=\"3\" style=\"text-align:center;\">{{ frappe.utils.format_date(doc.inspection_date_1, \"dd-MM-yyyy\") }}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{ frappe.utils.format_date(doc.inspection_date_2, \"dd-MM-yyyy\") }}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{ frappe.utils.format_date(doc.inspection_date_3, \"dd-MM-yyyy\") }}\n    </tr>\n    \n    <tr>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">TYPE OF LIFT</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">MRL</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">MR</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">PASSENGER</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">CAR</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">HOME</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">FREIGHT</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">DW</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">OVERHEAD</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">PIT</td>        \n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.mrl else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.mr else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.passenger else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.car else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.home else '&#x2610;' | safe }}</td>        \n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.freight else '&#x2610;' | safe }}</td>        \n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.dw else '&#x2610;' | safe }}</td>        \n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.over_head or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.pit or \"\"}}</td>        \n    </tr>\n    \n    <tr>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">TYPE OF DOORS</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">COLL</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">2PCO</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">2P TL / TR</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">VBP</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">4PCO</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">6PCO</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">8PCO</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">TRAVEL</td>        \n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;font-size:6px;\">SHAFT HEIGHT</td>        \n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.coll else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.pco2 else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.p_tltr else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.vbp else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.pco4 else '&#x2610;' | safe }}</td>        \n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.pco6 else '&#x2610;' | safe }}</td>        \n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.pco8 else '&#x2610;' | safe }}</td>        \n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.travel or \"\"}}</td>        \n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.shaft_height or \"\"}}</td>        \n    </tr>\n    \n    <tr>\n        <td rowspan=\"8\" style=\"background-color:#f3f3f3; text-align:center; vertical-align:middle;\">\n          <img src=\"/files/specifications.png\"\n               alt=\"SPECIFICATIONS\"\n               style=\"height:120px; display:block; margin:auto;\">\n        </td>\n        <td rowspan=\"8\" colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">\n            <img src=\"/files/data collection.png\"\n               alt=\"Data Collection\"\n               style=\"height:120px; display:block; margin:auto;\">\n        </td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">PAX</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">KG</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">SPEED (mps)</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">STOPS</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;\">FRONT OPENING</td>        \n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">REVERSE OPENING</td>        \n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;\">90 DEG OPENING</td>        \n             \n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.pax or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.kg or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.speed_mps or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.stops or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.front_opening or \"\"}}</td>        \n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.reverse_opening or \"\"}}</td>        \n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.deg_90_opening or \"\"}}</td>        \n    \n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">Roping 1:1</td>\n        <td colspan=\"1\" style=\"text-align:center;\">Roping 2:1</td>\n        <td colspan=\"1\" style=\"text-align:center;\">Roping 4:1</td>\n        <td colspan=\"1\" style=\"text-align:center; font-size:7px;\">MOTOR KW</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:7px;\">MOTOR MAKE</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:7px;\">MAIN ROPE DIA</td>  \n        <td colspan=\"1\" style=\"text-align:center;font-size:7px;\">NO OF ROPES</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:7px;\">OSG ROPE DIA</td>        \n        <td colspan=\"1\" style=\"text-align:center;font-size:7px;\">NO OF TRAV CABLE</td>        \n    \n    </tr>\n    <tr>\n    \n        <td colspan=\"2\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.roping_1_1 else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.roping_2_1 else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;\">{{ '&#x2611;' if doc.roping_4_1 else '&#x2610;' | safe }}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.motor_kw or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.motor_make or \"\"}}</td>  \n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.main_rope_dia or \"\"}}</td>        \n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.no_of_ropes or \"\"}}</td>        \n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.osg_rope_dia or \"\"}}</td>        \n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.no_of_trav_cable or \"\"}}</td>        \n    \n    </tr>\n    \n     </tr>\n    \n        <td colspan=\"2\" style=\"text-align:center;\">DOOR MAKE</td>\n        <td colspan=\"2\" style=\"text-align:center;\">DOOR CLEAR OPENING</td>\n        <td colspan=\"2\" style=\"text-align:center;\">DOOR CLEAR HEIGHT</td>\n        <td colspan=\"2\" style=\"text-align:center;\">ENCIDE MAKE</td>\n        <td colspan=\"2\" style=\"text-align:center;\">TRAV CABLE CORE</td>        \n          \n    \n    </tr>\n    \n    \n     </tr>\n    \n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.door_make or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.door_clear_opening or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.door_clear_height or \"\"}}</td>        \n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.encide_make or \"\"}}</td>        \n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.trav_cable_core or \"\"}}</td>        \n    \n    </tr>\n    \n    </tr>\n    \n        <td colspan=\"3\" style=\"text-align:center;\">Cop Type</td>\n        <td colspan=\"2\" style=\"text-align:center;\">DBR RATING</td>\n        <td colspan=\"1\" style=\"text-align:center;\">DRIVE KW</td>\n        <td colspan=\"2\" style=\"text-align:center;\">CONTROLLER MAKE</td>        \n        