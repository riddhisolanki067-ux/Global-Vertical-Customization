    \n    \n     <tr>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;font-weight:bold;\">SNO</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;font-weight:bold;\">DATE</td>\n        <td colspan=\"6\" style=\"text-align:center;background-color:#f3f3f3;font-weight:bold;\">Pending Work If Any</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#f3f3f3;font-weight:bold;\">CLEARED</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#f3f3f3;font-weight:bold;\">CLEARED DATE</td>\n            \n    </tr>\n    \n    {% for data in doc.pending_work %}\n    <tr>\n        <td colspan=\"1\" style=\"text-align:center;\">{{loop.index}}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{ frappe.utils.format_date(data.date, \"dd-MM-yyyy\") }}</td>\n        <td colspan=\"6\" style=\"text-align:center;\">{{data.points or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center; font-size:10px;\">{{ '&#x2611;' if data.cleared else '&#x2610;' | safe }}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{ frappe.utils.format_date(data.cleared_date, \"dd-MM-yyyy\") }}</td>\n    </tr>\n    {% endfor %}\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;background-color:black;font-weight:bold;border-right:1px solid white;color:white;\">DATE</td>\n        <td colspan=\"5\" style=\"text-align:center;background-color:black;font-weight:bold;border-right:1px solid white;color:white;\">SIGNATURE OF NI TEAM PRESENT DURING INSPECTION</td>\n        <td colspan=\"5\" style=\"text-align:center;background-color:black;font-weight:bold;color:white;font-size:7px;\">SIGNATURE OF EI TEAM PRESENT DURING INSPECTION</td>\n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">&nbsp</td>\n        <td colspan=\"5\" style=\"text-align:center;\"></td>\n        <td colspan=\"5\" style=\"text-align:center;\"></td></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">NAME</td>\n        <td colspan=\"5\" style=\"text-align:center;\"></td>\n        <td colspan=\"5\" style=\"text-align:center;\"></td></td>\n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;background-color:black;font-weight:bold;border-right:1px solid white;color:white;\">DATE</td>\n        <td colspan=\"5\" style=\"text-align:center;background-color:black;font-weight:bold;border-right:1px solid white;color:white;\">HANDOVER COMPLETED BY</td>\n        <td colspan=\"5\" style=\"text-align:center;background-color:black;font-weight:bold;color:white;\">I CONFIRMED THAT THIS LIFT ACCEPETED EI</td>\n    </tr>\n    \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">&nbsp</td>\n        <td colspan=\"5\" style=\"text-align:center;\"></td>\n        <td colspan=\"5\" style=\"text-align:center;\"></td>\n    </tr>\n  \n    <tr>\n        <td colspan=\"2\" style=\"text-align:center;\">NAME</td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;background-color:#f3f3f3;font-weight:bold;\">NI</td>\n        <td colspan=\"4\" style=\"text-align:center;\"></td>\n        <td colspan=\"1\" style=\"text-align:center;font-size:10px;background-color:#f3f3f3;font-weight:bold;\">EI</td>\n        <td colspan=\"4\" style=\"text-align:center;\"></td>\n    </tr>\n\n    <tr>\n        <td colspan=\"6\" style=\"text-align:center;font-size:10px;font-weight:bold;\">PLEASE FILL IN CAPITAL LETTERS</td>\n        <td colspan=\"6\" style=\"text-align:center;font-size:10px;font-weight:bold;\">PLEASE TAKE MANY PHOTOS AS YOU CAN</td>\n    </tr>\n\n   \n</table>\n",
