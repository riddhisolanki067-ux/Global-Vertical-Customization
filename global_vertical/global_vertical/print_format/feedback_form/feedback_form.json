 "font_size": 14,
 "html": "<table style=\"width: 100%; border-collapse: collapse; border: 1px solid #000; font-size: 12px;table-layout:fixed;text-align:center;\">\n    <!-- HEADER WITH LOGO AND TITLE -->\n    <tr>\n        <td rowspan=\"2\" colspan=\"3\" style=\"border: 1px solid #000; text-align: center; padding: 6px;\">\n            <img src=\"/files/easa_logo.png\" alt=\"EASA Logo\" style=\"height:40px;\">\n        </td>\n        <td rowspan=\"2\" colspan=\"7\" style=\"border: 1px solid #000; text-align: center; font-weight: bold; padding: 8px; font-size: 16px;\">\n            FEEDBACK FORM\n        </td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px; text-align: center;font-weight:bold;\">DATE</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px; text-align: center;\">{{ frappe.utils.format_date(doc.date, \"dd-MM-yyyy\") }}</td>\n    </tr>\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px; text-align: center;font-weight:bold;\">TIME</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px; text-align: center;\">{{doc.time or \"\"}}</td>\n    </tr>\n\n    <!-- NOTE -->\n    <tr>\n        <td colspan=\"16\" style=\"border: 1px solid #000; text-align: center; font-weight: bold; padding: 6px;\">\n            PLEASE WRITE IN CAPITAL LETTERS\n        </td>\n    </tr>\n\n    <!-- NAME & LOCATION -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">YOUR NAME</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.name1 or \"\"}}</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">YOUR LOCATION</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.location or \"\"}}</td>\n    </tr>\n\n    <!-- MOBILE & DEPARTMENT -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">MOBILE NUMBER</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.mobile_no or \"\"}}</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">DEPARTMENT</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.department or \"\"}}</td>\n    </tr>\n\n    <!-- DESIGNATION & YEARS IN ELEVATORS -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">DESIGNATION</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.designation or \"\"}}</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;font-size:10px;background-color:#f3f3f3;\">YEARS IN ELEVATORS</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.years_in_elevators or \"\"}}</td>\n    </tr>\n\n    <!-- OFFICIAL EMAIL & PCH -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">OFFICIAL EMAIL</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.official_email or \"\"}}</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;font-weight:bold;background-color:#f3f3f3;\">PCH</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.pch or \"\"}}</td>\n    </tr>\n\n    <!-- QUALIFICATIONS & YEARS WITH EASA -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">QUALIFICATIONS</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.qualifications or \"\"}}</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">YEARS WITH EASA</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.years_in_easa or \"\"}}</td>\n    </tr>\n\n    <!-- TRAINING & TRAINING LOCATION -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;background-color:#f3f3f3;\">TRAINING</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.training or \"\"}}</td>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;font-size:11px;background-color:#f3f3f3;\">TRAINING LOCATION</td>\n        <td colspan=\"5\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.training_location or \"\"}}</td>\n    </tr>\n\n    <!-- TRAINER -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;font-weight:bold;background-color:#f3f3f3;\">TRAINER</td>\n        <td colspan=\"13\" style=\"border: 1px solid #000; padding: 6px;\">{{doc.trainer or \"\"}}</td>\n    </tr>\n\n    <!-- KEY TAKEAWAY HEADER -->\n    <tr>\n        <td colspan=\"16\" style=\"border: 1px solid #000; text-align: center; font-weight: bold; padding: 6px;background-color:#f3f3f3;\">\n            KEY TAKEAWAY\n        </td>\n    </tr>\n\n    <!-- KEY TAKEAWAY LINES (1 to 13) -->\n    {% for data in doc.table_mzzq %}\n    <tr>\n        <td colspan=\"1\" style=\"border: 1px solid #000; text-align: center; padding: 6px;\">{{ loop.index }}</td>\n        <td colspan=\"15\" style=\"border: 1px solid #000; padding: 6px;\">{{data.data_ufzq}}</td>\n    </tr>\n    {% endfor %}\n\n    <!-- TRAINING SESSION HEADER -->\n    <tr>\n        <td colspan=\"16\" style=\"border: 1px solid #000; text-align: center; font-weight: bold; padding: 6px;background-color:#f3f3f3;\">\n            HOW DID YOU FIND THE TRAINING / SESSION\n        </td>\n    </tr>\n\n    <!-- TRAINING SESSION LINES (1 to 5) -->\n    {% for data in doc.table_ykpa %}\n    <tr>\n        <td colspan=\"1\" style=\"border: 1px solid #000; text-align: center; padding: 6px;\">{{ loop.index }}</td>\n        <td colspan=\"15\" style=\"border: 1px solid #000; padding: 6px;\">{{data.data_jxdm}}</td>\n    </tr>\n    {% endfor %}\n\n    <!-- SIGNATURE & DATE -->\n    <tr>\n        <td colspan=\"3\" style=\"border: 1px solid #000; padding: 6px;font-weight:bold;\">SIGNATURE</td>\n        <td colspan=\"7\" style=\"border: 1px solid #000; padding: 6px;font-weight:bold;\"></td>\n        <td colspan=\"2\" style=\"border: 1px solid #000; padding: 6px;font-weight:bold;\">DATE</td>\n        <td colspan=\"4\" style=\"border: 1px solid #000; padding: 6px;font-weight:bold;\">{{ frappe.utils.format_date(doc.date, \"dd-MM-yyyy\") }}</td>\n    </tr>\n\n    <!-- NOTE AT BOTTOM -->\n    <tr>\n        <td colspan=\"8\" style=\"border: 1px solid #000; padding: 6px; text-align: center; font-size: 11px;font-weight:bold;\">\n            PLEASE WRITE ON THE BACK SIDE IF SPACE IS SHORT.\n        </td>\n        <td colspan=\"8\" style=\"border: 1px solid #000; padding: 6px; text-align: center; font-size: 13px;font-weight:bold;\">\n            \u0915\u0943\u092a\u092f\u093e \u0938\u094d\u0925\u093e\u0928 \u0915\u092e \u0939\u094b\u0928\u0947 \u092a\u0930 \u092a\u0940\u091b\u0947 \u092d\u0940 \u0914\u0930 \u0932\u093f\u0916\u0947\u0902\u0964\n        </td>\n    </tr>\n</table>\n",
