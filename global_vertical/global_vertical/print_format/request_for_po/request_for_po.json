 "html": "<table style=\"width:100%; border-collapse: collapse; table-layout: fixed;\" border=\"1\">\n    <!-- HEADER -->\n    <tr>\n        <td colspan=\"16\" style=\"background-color:#666666;text-align:center;border: 1px solid #000;\"></td>\n    </tr>\n    <tr>\n        <td rowspan=\"2\" colspan=\"3\" style=\"text-align:center;\">\n            <img src=\"/files/global_logo.png\" alt=\"Logo\" style=\"height:40px;\">\n        </td>\n        <td colspan=\"8\" style=\"text-align:center; font-weight:bold; background:yellow;font-size:12px;\">\n            REQUEST FOR PO FOR SUB-CONTRACTOR\n            </td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;\">SEQ NO</td>\n        <td colspan=\"3\" style=\"text-align:center;\">{{doc.seq_no or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"6\" style=\"text-align:center;\">\n            FOR HIRING A SUB-CONTRACTOR\n            </td>\n        <td colspan=\"7\" style=\"text-align:center;\">PLEASE FILL IN CAPITAL LETTERS</td>\n\n    </tr>\n    \n     <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">DATE</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{ frappe.utils.format_date(doc.date, \"dd-MM-yyyy\") }}</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">Branch</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.branch or \"\"}}</td>\n\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;\">Order No</td>\n\n        <td colspan=\"5\" style=\"text-align:center;\">{{doc.order_no or \"\"}}</td>\n\n    </tr>\n    \n     <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">SITE NAME</td>\n        <td colspan=\"6\" style=\"text-align:center;\">{{doc.site_name or \"\"}}</td>\n\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;\">NO OF LIFTS</td>\n\n        <td colspan=\"5\" style=\"text-align:center;\">{{doc.no_of_lifts or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">SITE ADDRESS</td>\n        <td colspan=\"6\" style=\"text-align:center;\">{{doc.site_address or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;\">PIN CODE</td>\n        <td colspan=\"5\" style=\"text-align:center;\">{{doc.pin_code or \"\"}}</td>\n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">SCOPE OF WORK</td>\n        <td colspan=\"13\" style=\"text-align:center;\">{{doc.scope_of_work or \"\"}}</td>\n     \n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">EXTRA WORK IF ANY</td>\n        <td colspan=\"13\" style=\"text-align:center;\">{{doc.extra_work_if_any or \"\"}}</td>\n     \n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">INSTALLATION START DATE</td>\n        <td colspan=\"4\" style=\"text-align:center;\">{{ frappe.utils.format_date(doc.installation_start_date, \"dd-MM-yyyy\") }}</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#efefef;font-weight:bold;\">INSTALLATION END DATE</td>\n        <td colspan=\"7\" style=\"text-align:center;\">{{ frappe.utils.format_date(doc.installation_end_date, \"dd-MM-yyyy\") }}</td>\n     \n    </tr>\n    \n    <tr>\n        <td rowspan=\"2\" colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">TYPE OF LIFT</td>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;\">{{doc.type_of_lift or \"\"}}</td>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">TYPE OF DRIVE</td>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;\">{{doc.type_of_drive or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;background-color:#efefef;font-weight:bold;font-size:9px;\">STOPS</td>\n        \n        <td colspan=\"2\" style=\"text-align:center;background-color:#efefef;font-weight:bold;font-size:9px;\">FRONT OPENING</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#efefef;font-weight:bold;font-size:9px;\">90 DEG OPENING IF ANY</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#efefef;font-weight:bold;font-size:9px;\">REVERSE OPENING IF ANY</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"1\" style=\"text-align:center;font-size:9px;\">{{doc.stops or \"\"}}</td>\n        \n        <td colspan=\"2\" style=\"text-align:center;font-size:9px;\">{{doc.front_opening or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;font-size:9px;\">{{doc.deg_90_opening_if_any or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;font-size:9px;\">{{doc.reverse_opening_if_any or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">TYPE OF LIFT</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.type_of_lift or \"\"}}</td>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">ROPING</td>\n        <td colspan=\"8\" style=\"text-align:center;\">{{doc.roping or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td rowspan=\"2\" colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">SPECS</td>\n        <td colspan=\"1\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">Pax</td>\n        <td colspan=\"1\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">Kg</td>\n        <td colspan=\"1\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">SPEED</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">OVER HEAD</td>\n        <td colspan=\"2\" style=\"text-align:center;background-color:#efefef;font-weight:bold;font-size:9px;\">PIT</td>\n        \n        <td colspan=\"3\" style=\"text-align:center;background-color:#efefef;font-weight:bold;font-size:9px;\">TRAVEL</td>\n        <td colspan=\"3\" style=\"text-align:center;background-color:#efefef;font-weight:bold;font-size:9px;\">SHAFT HEIGHT</td>\n\n    </tr>\n    \n     <tr>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.pax or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.kg or \"\"}}</td>\n        <td colspan=\"1\" style=\"text-align:center;\">{{doc.speed or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.over_head or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:center;\">{{doc.pit or \"\"}}</td>\n        <td colspan=\"3\" style=\"text-align:center;\">{{doc.travel or \"\"}}</td>\n        <td colspan=\"3\" style=\"text-align:center;\">{{doc.shaft_height or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"16\" style=\"background-color:black;text-align:center;border: 1px solid #000;font-size:10px;font-weight:bold;color:white;\">SUB-CONTRACTOR DETAILS</td>\n    </tr>\n    \n     <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold; background:yellow;\">\n            NAME OF SUB CONTRACTOR\n        </td>\n        <td colspan=\"8\" style=\"text-align:center; font-weight:bold; background:yellow;font-size:12px;\">\n          {{doc.name_of_sub_contractor or \"\"}}\n            </td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;font-weight:bold; background:yellow;\">MOBILE</td>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold; background:yellow;\">{{doc.mobile or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">PAN NO</td>\n        <td colspan=\"5\" style=\"text-align:center;font-size:9px;\">{{doc.pan_no or \"\"}}</td>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">AADHAR CARD NO</td>\n        <td colspan=\"5\" style=\"text-align:center;font-size:9px;\">{{doc.aadhar_card_no or \"\"}}</td>\n    </tr>\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;\">CONTRACTOR ADDRESS</td>\n        <td colspan=\"13\" style=\"text-align:center;font-size:9px;\">{{doc.contractor_address or \"\"}}</td>\n        \n    </tr>\n    \n    \n     <tr>\n        <td colspan=\"1\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">SNO</td>\n        <td colspan=\"4\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">SPECIFICATION</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">CUSTOMER LIFT NOs</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">RATE PER LIFT</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">NO OF LIFTS</td>\n        <td colspan=\"2\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">REQUESTED AMOUNT</td>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">APPROVED AMOUNT</td>\n    </tr>\n    \n    {% set ns = namespace(total_requested=0, total_approved=0) %}\n    \n    {% for data in doc.sub_contractor %}\n        {% set ns.total_requested = ns.total_requested + (data.requested_amount or 0) %}\n        {% set ns.total_approved = ns.total_approved + (data.approved_amount or 0) %}\n    \n        <tr>\n            <td rowspan=\"2\" colspan=\"1\" style=\"text-align:center;font-size:9px;\">{{ loop.index }}</td>\n            <td rowspan=\"1\" colspan=\"4\" style=\"text-align:center;font-size:9px;\">{{ data.specification or \"\" }}</td>\n            <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-size:9px;\">{{ data.customer_lift_nos or \"\" }}</td>\n            <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-size:9px;\">{{ data.rate_per_lift or \"\" }}</td>\n            <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-size:9px;\">{{ data.no_of_lifts or \"\" }}</td>\n            <td rowspan=\"2\" colspan=\"2\" style=\"text-align:center;font-size:9px;\">{{ data.requested_amount or \"\" }}</td>\n            <td rowspan=\"2\" colspan=\"3\" style=\"text-align:center;font-size:9px;\">{{ data.approved_amount or \"\" }}</td>\n        </tr>\n        <tr>\n            <td rowspan=\"1\" colspan=\"4\" style=\"text-align:center;font-size:9px;\">{{ data.specification_1 or \"\" }}</td>\n        </tr>\n    {% endfor %}\n    \n    <tr>\n        <td colspan=\"1\" style=\"text-align:center;font-size:9px;\">&nbsp;</td>\n        <td colspan=\"4\" style=\"text-align:center;font-size:9px;\"></td>\n        <td colspan=\"6\" style=\"text-align:right;font-size:9px; font-weight:bold;\">TOTAL</td>\n        <td colspan=\"2\" style=\"text-align:center;font-size:9px; font-weight:bold;\">\n            {{ \"{:,.2f}\".format(ns.total_requested) }}\n        </td>\n        <td colspan=\"3\" style=\"text-align:center;font-size:9px; font-weight:bold;\">\n            {{ \"{:,.2f}\".format(ns.total_approved) }}\n        </td>\n    </tr>\n\n    \n    <tr>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">PAYMENT TERMS</td>\n        <td colspan=\"7\" style=\"text-align:center;font-size:9px;\">{{doc.payment_terms or \"\"}}</td>\n        <td colspan=\"3\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;\">NO OF WORKERS</td>\n        <td colspan=\"3\" style=\"text-align:center;font-size:9px;\">{{doc.no_of_workers or \"\"}}</td>\n\n    </tr>\n    \n    <tr>\n        <td colspan=\"16\" style=\"text-align:center;font-size:9px;\">PLEASE ATTACH SUB CONTRACTOR BANKS CANCEL CHEQUES, COPY OF PAN CARD & AADHAR CARD</td>\n    </tr>\n    \n    <tr>\n        <td colspan=\"5\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;padding: 6px !important;\">REQUESTED BY</td>\n        <td colspan=\"5\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;padding: 6px !important;\">CHECKED BY</td>\n        <td colspan=\"6\" style=\"text-align:center;font-weight:bold;background-color:#efefef;font-size:9px;padding: 6px !important;\">APPROVED BY</td>\n    </tr>  \n    \n    <tr>\n        <td colspan=\"5\" style=\"text-align:center;font-size:9px;\">PLEASE ATTACH OF COPY OF GAD</td>\n        <td colspan=\"5\" style=\"text-align:center;font-size:9px;\">PLEASE ATTACH OF COPY OF GAD</td>\n        <td colspan=\"6\" style=\"text-align:center;font-size:9px;\">PLEASE ISSUE PO</td>\n    </tr>  \n    \n    <tr>\n        <td rowspan=\"2\" colspan=\"5\" style=\"text-align:center;font-size:9px;padding: 6px !important;\"></td>\n        <td colspan=\"5\" style=\"text-align:center;font-size:9px;padding: 6px !important;\">&nbsp</td>\n        <td rowspan=\"2\" colspan=\"6\" style=\"text-align:center;font-size:9px;padding: 6px !important;\"></td>\n    </tr> \n    \n    <tr>\n        <td colspan=\"5\" style=\"text-align:center;font-weight:bold;font-size:9px;padding: 6px !important;\">COORDINATE</td>\n    </tr> \n    \n    \n    \n    \n    \n    \n    \n    \n    \n    \n    \n</table>",
